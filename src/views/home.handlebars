<div class="container mt-5">
    <h1 class="text-center mn-4">LISTA DE PRODUCTOS</h1>
        <div class="row row-cols-1 row-cols-md-2 row-cols-lg-3">
            {{#each products}}
                <div class="col mb-4">
                    <div class="card h-100 bg-light">
                        <div class="card-heard bg-success text-white">
                            <i class="bi bi-tag">ID:{{this.id}}</i>
                        </div>
                            <div class="card-body bg-light">
                                    
                                    <ul class="list-unstyled">
                                        <li>Titulo:{{this.title}}</li>
                                        <li>Description:{{this.description}}</li>
                                        <li>Precio:${{this.price}}</li>
                                        <li>Stock:{{this.stock}}</li>
                                        <li>Status:{{this.status}}</li>
                                        <li>Categoria:{{this.category}}</li>
                                        <li>Codigo:{{this.code}}</li>
                                        <li>
                                            <img>Img:{{this.imagen}}</img>
                                        </li>

                                    </ul>
                            </div>  
                    </div>
                </div>
            {{/each}}
        </div>
</div>    

